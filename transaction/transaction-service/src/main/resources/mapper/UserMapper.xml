<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="net.codingw.rocketmq.learning.transaction.user.mapper.UserMapper">

    <resultMap id="BaseUserMap" type="net.codingw.rocketmq.learning.transaction.user.model.User">
        <id column="id" property="id" jdbcType="INTEGER" />
        <result column="password" property="password" jdbcType="VARCHAR" />
        <result column="username" property="username" jdbcType="VARCHAR" />
        <result column="sex" property="sex" jdbcType="INTEGER" />
        <result column="email" property="email" jdbcType="VARCHAR" />
        <result column="createTime" property="create_time" jdbcType="TIMESTAMP" />
        <result column="updateTime" property="update_time" jdbcType="TIMESTAMP" />
    </resultMap>

</mapper>